


public class Produto {
	protected long ID;
	protected String Nome;
	protected String Descricao;
	protected static double PrecoUnitario;
	protected static int QuantidadeEstoque;
	protected Categoria Categoria;
	protected static double Imposto;
	protected static double TotalEstoque;
	
	public static double CalculaValorEstoque() {
		TotalEstoque = (PrecoUnitario*QuantidadeEstoque);
		return TotalEstoque;
	}
	
	public static double CalculaImposto() {
		Imposto = (PrecoUnitario*0.4);
		return Imposto;
		 
	}


	
	
}

	