package Teste;

import br.com.comex.modelo.StatusCategoria;

public class TesteMainCategoria {


	public static void main(String[] args) {
		try { 
		TesteCategoria categoria0 = new TesteCategoria("LAAU");
		System.out.println("Categoria " + categoria0.getNome() + " ( " + categoria0.getID() + " - "
				+ categoria0.getStatus() + ")");
	} catch (IllegalArgumentException ex){
		System.out.println("O seguinte item foi inserido incorretamente: " + ex.getMessage());
	}
}
