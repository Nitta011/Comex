import java.text.SimpleDateFormat;
import java.util.Date;


public class Pedido {
	int ID;
	Date Data;
	String Cliente;

	public int getID() {
		return ID;
	}
	public Date getData() {
		return Data;
	}
	public String getCliente() {
		return Cliente;
	}
	SimpleDateFormat formatter = new SimpleDateFormat("dd/MM/yyyy");
}
