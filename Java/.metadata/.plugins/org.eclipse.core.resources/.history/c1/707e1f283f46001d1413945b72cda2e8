package br.com.comex.csv;

import java.util.Comparator;

public class PedidoMaisBaratoEMaisCaro implements Comparator<PedidosCsv> {

	@Override
	public int compare(PedidosCsv p1, PedidosCsv p2) {
		
		double precoP1 = Double.parseDouble(p1.getPreco());
		double precoP2 = Double.parseDouble(p2.getPreco());
		
		if(precoP1 == precoP2) {
			return 0;
		} else if (precoP1 > precoP2){
			return 1;
		} else {
			return -1;
		}
	}

}
