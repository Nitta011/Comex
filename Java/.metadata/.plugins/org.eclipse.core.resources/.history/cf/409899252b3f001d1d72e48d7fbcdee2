package Teste;

import br.com.comex.modelo.Categoria;
import br.com.comex.modelo.Produto;
import br.com.comex.modelo.ProdutoIsento;

public class TesteProdutoIsento extends TesteProduto {

	public TesteProdutoIsento(int ID, String Nome, String Descricao, 
			double PrecoUnitario, int QuantidadeEstoque,
			TesteCategoria Categoria) {
		
			super(ID, Nome, Descricao, PrecoUnitario, QuantidadeEstoque, Categoria);
			
		}

	public static double ImpostoIsento() {
		Imposto = 0.0;
		return Imposto;
	}

	public static double CalculaValorEstoqueIsento() {
		TotalEstoque = (ProdutoIsento.getPrecoUnitario() * ProdutoIsento.getQuantidadeEstoque());
		return TotalEstoque;
	}

}
