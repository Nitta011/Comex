
public class Categoria{

	private static long ID;
	private static String Nome;
	private static StatusCategoria status = StatusCategoria.ATIVA;
	
	public Categoria() {
		this.ID = ID;
		this.Nome = Nome;
		this.status = status;
		
	}

	public static void main(String[] args) 
	 {
		try {
			String Nome = "Laua";
			int NumeroDeCaracteresNome = Nome.length();
			NumeroDeCaracteresNome = 3;
			ID = 0;	
			} catch (IllegalArgumentException | NullPointerException ex) {
			System.out.println("Deu merda");	
			}	
		
	}
	
	public long getID() {
		return ID;
	}

	public static void setID(long iD) {
		ID = iD;
	}

	public static String getNome() {
		return Nome;
	}

	public static void setNome(String nome) {
		Nome = nome;
	}

	public StatusCategoria getStatus() {
		return status;
	}

	public static void setStatus(StatusCategoria status) {
		status = status;
	}
	
}