
	public class Teste extends Produto{
		public Teste(int ID, String Nome, String Descricao, double PrecoUnitario, int QuantidadeEstoque, String Categoria) {
			super(ID, Nome, Descricao, PrecoUnitario, QuantidadeEstoque, Categoria);
		}

		public double CalculaImposto() {
			Produto.PrecoUnitario = 289.22;
			Imposto = (PrecoUnitario * 0.40);
			System.out.println("O Imposto obtido em cima desse produto Ã© de: " + Imposto);
			return Imposto;

	}
}
