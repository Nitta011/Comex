	}

	public static void CalculaImposto() {
		Produto produto = new Produto(0, null, null, 198.22, 1, null);
		Produto.Imposto = (Produto.PrecoUnitario * 0.40);
		System.out.println("O Imposto obtido em cima desse produto Ã© de: " + Imposto);

	}
}
